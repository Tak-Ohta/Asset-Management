<%= provide(:title, "ユーザー一覧") %>
<h2>ユーザー一覧</h2>

<div class="col-md-12">
  <table class="table table-hover" id="table-users">
    <thead>
      <tr>
        <th>名前</th>
        <th>生年月日</th>
        <th>性別</th>
        <th>住まい</th>
        <th>職業</th>
        <th>未婚/既婚</th>
        <th>年収</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.name %></td>
          <td><%= user.birthday %></td>
          <td><%= user.sex %></td>
          <td><%= user.prefecture %></td>
          <td><%= user.occupation %></td>
          <td><%= user.marriage %></td>
          <td><%= user.annual_income %></td>
        </tr>
      <%end %>
    </tbody>
  </table>
</div>
